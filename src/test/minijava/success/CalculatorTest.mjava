class Calculator {
    private int value;

    public Calculator() {
        this.value = 0;
    }

    public int add(int x) {
        this.value = this.value + x;
        return this.value;
    }
}

public class CalculatorTest {
    public static void main(String[] args) {
        Calculator calc = new Calculator();
        print calc.add(5);
    }
}