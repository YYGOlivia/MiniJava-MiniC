{
    "search.exclude": {
        "**/*.class": true, // Exclude all class files from search
        "target": true, // Exclude target folder from search
        ".antlr": true, // Exclude ANTLR generated files
    },
    "files.associations": {
        "*.tam": "lisp",
        "*.block": "c",
        "*.mjava": "java"
    },
    "java.project.referencedLibraries": [
        "tools/antlr-4.13.1-complete.jar",
        "tools/commons-lang3-3.12.0.jar",
        "tools/commons-text-1.9.jar"
    ]
}